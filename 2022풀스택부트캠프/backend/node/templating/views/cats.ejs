<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>All Cats</title>
  </head>
  <body>
    <h1>All The cats</h1>
    <ul>
      <!-- for of문 -->
      <% for(let cat of cats){ %>
      <li><%=cat%></li>
      <%} %>

      <!-- map 사용 가능 -->
      <% cats.map(ev => { %>
      <li><%=ev%></li>
      <%}) %>
    </ul>
  </body>
</html>
